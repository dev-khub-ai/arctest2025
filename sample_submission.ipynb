        if i < len(test):
            ar[2,i].imshow(np.array(test[i][0]['input']), cmap=cmap, norm=norm)
        else:
            ar[2,i].imshow(np.zeros(np.array(test[0][0]['input']).shape), cmap=cmap, norm=norm)
    plt.show()
