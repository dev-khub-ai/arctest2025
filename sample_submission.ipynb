def mask_match(task):
    result = True
    for i in range(len(task)):
        if not np.array_equal(np.clip(np.array(task[i]['input']), 0, 1), np.clip(np.array(task[i]['output']), 0, 1)):
            result = False
            break
    return result

def shape_match(task): 
    result = True
    for i in range(len(task)):
        if not np.array_equal(np.array(task[i]['input']).shape, np.array(task[i]['output']).shape):
            result = False
            break
    return result

def size_match(task): 
    result = False
    uniques = {}
    s = []
    for i in range(len(task)):
        s.append(np.array(task[i]['input']).shape)
        s.append(np.array(task[i]['output']).shape)
    for u in s:
        if u in uniques:
            uniques[u]+=1
        else:
            uniques[u]=1
    if len(uniques) == 1:
        result = True
    return result

def rotation_match(task): 
    r_ = {r:0 for r in range(1,4)}
    result = 0
    for i in range(len(task)):
        for r in range(3):
            rot = np.rot90(np.array(task[i]['input']), r+1)
            #rot = np.flip(rot, axis=1)
            if np.array_equal(np.array(task[i]['output']), rot):
                r_[r + 1] += 1
    for r in r_:
        if r_[r]==len(task):
            result = r
    return result

def color_match(task): #should validate task group not individual task
    result = True
    for i in range(len(task)):
        if not np.array_equal(np.unique(np.array(task[i]['input'])), np.unique(np.array(task[i]['output']))):
            result = False
            break
    return result

def sizes(task): 
    result = []
    for i in range(len(task)):
        result.append([np.array(task[i]['input']).shape, np.array(task[i]['output']).shape])
    return result

def fcolors(task): 
    result = []
    for i in range(len(task)):
        result.append([set(np.array(task[i]['input']).flatten()), set(np.array(task[i]['output']).flatten())])
    return result

def vcolors(task): 
    result = []
    for i in range(len(task)):
        inp = pd.DataFrame(np.array(task[i]['input']).flatten()).value_counts()
        inp = {a:b for a,b in inp.reset_index().values}
        outp = pd.DataFrame(np.array(task[i]['output']).flatten()).value_counts()
        outp = {a:b for a,b in outp.reset_index().values}
        result.append([inp, outp])
    return result

dfall['mask_match'] = dfall['train'].map(lambda x: mask_match(x))
dfall['shape_match'] = dfall['train'].map(lambda x: shape_match(x))
dfall['rotation_match'] = dfall['train'].map(lambda x: rotation_match(x))
dfall['size_match'] = dfall['train'].map(lambda x: size_match(x))
dfall['color_match'] = dfall['train'].map(lambda x: color_match(x))
dfall['sizes'] = dfall['train'].map(lambda x: sizes(x))
dfall['colors'] = dfall['train'].map(lambda x: fcolors(x))
dfall['vcolors'] = dfall['train'].map(lambda x: vcolors(x))
dfall.to_csv('ARC 2025.csv', index=False)



